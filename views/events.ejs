  /* ---------------------------------------timeline for events -------------------------- */
  body {
    background-color: #f7f7f7;
    margin-top: 20px;
    color: #ffffff;
  }

  .main-timeline {
    position: relative;
  }

  .main-timeline:before {
    content: "";
    display: block;
    width: 2px;
    height: 100%;
    background: #c6c6c6;
    margin: 0 auto;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
  }

  .main-timeline .timeline {
    margin-bottom: 40px;
    position: relative;
  }

  .main-timeline .timeline:after {
    content: "";
    display: block;
    clear: both;
  }

  .main-timeline .icon {
    width: 18px;
    height: 18px;
    line-height: 18px;
    margin: auto;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
  }

  .main-timeline .icon:before,
  .main-timeline .icon:after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: 50%;
    position: absolute;
    top: 0;
    left: 0;
    transition: all 0.33s ease-out 0s;
  }

